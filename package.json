{
  "name": "wholestack",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node dist/index.js",
    "dev2B": "npm-run-all -p dev:server dev:2B",
    "dev:server": "nodemon --watch server --exec npm-run-all -s dev:server:build dev:server:exec",
    "dev:server:build": "cross-env NODE_ENV=development webpack",
    "dev:server:exec": "node dist/index.js",
    "dev:2B": "cd 2B && npm run start",
    "build": "npm-run-all -s build:server build:2B",
    "build:server": "cross-env NODE_ENV=production webpack",
    "build:2B": "cd 2B && npm run build"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cross-env": "^7.0.3",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "^8.1.1",
    "npm-run-all": "^4.1.5"
  }
}
